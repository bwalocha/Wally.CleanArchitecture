{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FeatureManagement": {
    "SwaggerDisabled": false
  },
  "OpenTelemetry": {
    "Endpoint": "http://aspire:18888"
  },
  "Cors": {
    "Origins": [
      "http://localhost:3000",
      "http://localhost:8080"
    ]
  },
  "Authentication": {
    "Authority": "https://accounts.google.com",
    "ClientId": "---.apps.googleusercontent.com",
    "ClientSecret": "---"
  },
  "SwaggerAuthentication": {
    "Authority": "https://accounts.google.com",
    "ClientId": "---.apps.googleusercontent.com",
    "ClientSecret": "---"
  },
  "HealthChecksUI": {
    "DisableMigrations": true,
    "HealthChecks": [
      {
        "Name": "self",
        "Uri": "https://localhost:5001/healthChecks"
      },
      {
        "Name": "Wally.MicroService",
        "Uri": "https://localhost:5001/MicroService-api/healthChecks"
      }
    ],
    "_Webhooks": [
      {
        "Name": "",
        "Uri": "",
        "Payload": "",
        "RestoredPayload": ""
      }
    ],
    "EvaluationTimeInSeconds": 600,
    "MinimumSecondsBetweenFailureNotifications": 600,
    "MaximumExecutionHistoriesPerEndpoint": 100,
    "HealthCheckDatabaseConnectionString": "Data Source=healthChecks"
  },
  "ReverseProxy": {
    "Routes": {
      "MicroService-health": {
        "ClusterId": "MicroService-api",
        "AuthorizationPolicy": "Anonymous",
        "Match": {
          "Path": "/MicroService-api/healthChecks"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/MicroService-api"
          }
        ]
      },
      "MicroService": {
        "ClusterId": "MicroService-api",
        "AuthorizationPolicy": "AuthenticatedUsers",
        "CorsPolicy ": "CorsPolicy",
        "Match": {
          "Path": "/MicroService-api/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/MicroService-api"
          }
        ]
      }
    },
    "Clusters": {
      "MicroService-api": {
        "Destinations": {
          "primary": {
            "Address": "https://localhost:7181"
          }
        }
      }
    }
  }
}
