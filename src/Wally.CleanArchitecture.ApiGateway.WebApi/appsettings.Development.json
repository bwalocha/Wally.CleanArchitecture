{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "Origins": [
      "http://localhost:3000",
      "http://localhost:8080"
    ]
  },
  "ReverseProxy": {
    "Routes": {
      "MicroService": {
        "ClusterId": "MicroService-api",
        "Match": {
          "Path": "/MicroService-api/{*any}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/MicroService-api"
          }
        ]
      }
    },
    "Clusters": {
      "MicroService-api": {
        "Destinations": {
          "primary": {
            "Address": "https://localhost:7181"
          }
        }
      }
    }
  }
}
